#include "tegra234-camera-ar0233-a00_sensing.dtsi"
#include "dt-bindings/clock/tegra234-clock.h"


#define CAM0_PWDN	TEGRA234_MAIN_GPIO(H, 6)
#define CAM1_PWDN	TEGRA234_MAIN_GPIO(AC, 0)
#define CAM_PWM	TEGRA234_MAIN_GPIO(G, 6)
#define CAMERA_I2C_MUX_BUS(x) (0x1E + x)

/* camera control gpio definitions */

/ {
		gpio@2200000 {
	
		camera-control-output-CAM0-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM0_PWDN 0>;
                        output-high;
                        label = "cam0-pwdn";
                };
		camera-control-output-CAM1-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM1_PWDN 0>;
                        output-high;
                        label = "cam1-pwdn";
                };
                camera-control-output-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM_PWM 0>;
                        output-high;
                        label = "cam-pwm";
                };
	};
	i2c@3180000 {
		tca9543@72 {
			compatible = "nxp,pca9543";
			reg = <0x72>;
			#address-cells = <1>;
			#size-cells = <0>;
			skip_mux_detect = "yes";
			force_bus_start = <CAMERA_I2C_MUX_BUS(0)>;
			i2c-mux-idle-disconnect;
			i2c@0 {
				reg = <0>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
				i2c-mux-idle-disconnect;
				dser: max9296@48 {
					compatible = "sensing,max9296";
					reg = <0x48>;
					csi-mode = "2x4";
					max-src = <2>;
					//fsync-gpio = <6>; //fsync gpio on 9296
					//reset-gpios = <&tegra_main_gpio CAM0_PWDN GPIO_ACTIVE_HIGH>;
				};
				ser_prim: max9295_prim@40 {
					compatible = "sensing,max9295";
					reg = <0x40>;
					is-prim-ser;
				};
				ser_a: max9295_a@42 {
					compatible = "sensing,max9295";
					reg = <0x42>;
					nvidia,gmsl-dser-device = <&dser>;
				};
				ser_b: max9295_b@60 {
					compatible = "sensing,max9295";
					reg = <0x60>;
					nvidia,gmsl-dser-device = <&dser>;
				};
				cam0@1b {
					reg = <0x1b>;
					
					/* Define any required hw resources needed by driver */
					/* ie. clocks, io pins, power sources */
					clocks = <&bpmp_clks TEGRA234_CLK_EXTPERIPH1>,
							<&bpmp_clks TEGRA234_CLK_EXTPERIPH1>;
					clock-names = "extperiph1", "pllp_grtba";
					mclk = "extperiph1";
					nvidia,gmsl-ser-device = <&ser_a>;
					nvidia,gmsl-dser-device = <&dser>;
				};
				cam1@1c {
					reg = <0x1c>;
					
					/* Define any required hw resources needed by driver */
					/* ie. clocks, io pins, power sources */
					clocks = <&bpmp_clks TEGRA234_CLK_EXTPERIPH1>,
							<&bpmp_clks TEGRA234_CLK_EXTPERIPH1>;
					clock-names = "extperiph1", "pllp_grtba";
					mclk = "extperiph1";
					nvidia,gmsl-ser-device = <&ser_b>;
					nvidia,gmsl-dser-device = <&dser>;
				};
			};
			i2c@1 {
				reg = <1>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
				i2c-mux-idle-disconnect;
				dsera: max9296@48 {
					compatible = "sensing,max9296";
					reg = <0x48>;
					csi-mode = "2x4";
					max-src = <2>;
					//reset-gpios = <&tegra_main_gpio CAM1_PWDN GPIO_ACTIVE_HIGH>;
                                        //fsync-gpio = <6>; //fsync gpio on 9296
				};
				ser_prima: max9295_prim@40 {
					compatible = "sensing,max9295";
					//streams = "yuv8";
					reg = <0x40>;
					is-prim-ser;
				};
				ser_c: max9295_a@42 {
					compatible = "sensing,max9295";
					reg = <0x42>;
					nvidia,gmsl-dser-device = <&dsera>;
				};
				ser_d: max9295_b@60 {
					compatible = "sensing,max9295";
					reg = <0x60>;
					nvidia,gmsl-dser-device = <&dsera>;
				};
				cam2@1b{
					reg = <0x1b>;
						
					clocks = <&bpmp_clks TEGRA234_CLK_EXTPERIPH1>,
							<&bpmp_clks TEGRA234_CLK_EXTPERIPH1>;
					clock-names = "extperiph1", "pllp_grtba";
					mclk = "extperiph1";
					nvidia,gmsl-ser-device = <&ser_c>;
					nvidia,gmsl-dser-device = <&dsera>;
				};
				cam3@1c {
					reg = <0x1c>;
						
					clocks = <&bpmp_clks TEGRA234_CLK_EXTPERIPH1>,
							<&bpmp_clks TEGRA234_CLK_EXTPERIPH1>;
					clock-names = "extperiph1", "pllp_grtba";
					mclk = "extperiph1";
					nvidia,gmsl-ser-device = <&ser_d>;
					nvidia,gmsl-dser-device = <&dsera>;
				};
			};

		};
	};
};
