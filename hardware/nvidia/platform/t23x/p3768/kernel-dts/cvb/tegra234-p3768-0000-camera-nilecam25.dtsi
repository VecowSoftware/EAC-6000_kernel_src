/* Econ device tree for AR0234 support */

#include "dt-bindings/clock/tegra234-clock.h"

#define CAM0_PWDN	TEGRA234_MAIN_GPIO(H, 6)
#define CAM1_PWDN	TEGRA234_MAIN_GPIO(AC, 0)
#define CAM2_PWDN	TEGRA234_MAIN_GPIO(H, 3)
#define CAM3_PWDN	TEGRA234_MAIN_GPIO(AC, 1)
#define CAM_PWM		TEGRA234_MAIN_GPIO(M, 7)
#define CAM_I2C_MUX 	TEGRA234_AON_GPIO(CC, 3)
#define CAMERA_I2C_MUX_BUS(x) (0x1E + x)

#define CAM_LANES 2
#define CAM_LANES_STRING "2"

/ {
	gpio@2200000 {
	
		camera-control-output-CAM0-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM0_PWDN 0>;
                        output-high;
                        label = "cam0-pwdn";
                };
		camera-control-output-CAM1-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM1_PWDN 0>;
                        output-high;
                        label = "cam1-pwdn";
                };
                camera-control-output-CAM2-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM2_PWDN 0>;
                        output-high;
                        label = "cam2-pwdn";
                };
                camera-control-output-CAM3-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM3_PWDN 0>;
                        output-high;
                        label = "cam3-pwdn";
                };
                camera-control-output-PWM-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM_PWM 0>;
                        output-high;
                        label = "cam-pwm";
                };
	};

capture_vi_base: tegra-capture-vi  {
			num-channels = <8>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";
			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				status = "okay";
				port@0 {
					reg = <0>;
					status = "okay";
					ecam_vi_in0: endpoint {
						vc-id = <0x01>;
						status = "okay";
						port-index = <0>;
						bus-width = <CAM_LANES>;
						remote-endpoint = <&ecam_csi_out0>;
					};
				};
				port@1 {
                                        reg = <1>;
					status = "okay";
                                        ecam_vi_in1: endpoint {
                                        	vc-id = <0x00>;
						status = "okay";
                                                port-index = <1>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out1>;
                                        };
                                };
                                port@2 {
                                        reg = <2>;
					status = "okay";
                                        ecam_vi_in2: endpoint {
                                        	vc-id = <0x01>;
						status = "okay";
                                                port-index = <2>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out2>;
                                        };
                                };
				port@3 {
                                        reg = <3>;
					status = "okay";
                                        ecam_vi_in3: endpoint {
						status = "okay";
						vc-id = <0x00>;
                                                port-index = <3>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out3>;
                                        };
                                };
				port@4 {
                                        reg = <4>;
					status = "okay";
                                        ecam_vi_in4: endpoint {
                                        	vc-id = <0x01>;
						status = "okay";
                                                port-index = <4>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out4>;
                                        };
                                };
				port@5 {
                                        reg = <5>;
					 status = "okay";
                                        ecam_vi_in5: endpoint {
						status = "okay";
						vc-id = <0x00>;
                                                port-index = <4>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out5>;
                                        };
                                };
				port@6 {
                                        reg = <6>;
