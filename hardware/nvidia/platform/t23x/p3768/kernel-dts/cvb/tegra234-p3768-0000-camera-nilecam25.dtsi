						vc-id = <0x01>;
                                                port-index = <5>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out6>;
                                        };
                                };
				port@7 {
                                        reg = <7>;
					status = "okay";
                                        ecam_vi_in7: endpoint {
						status = "okay";
						vc-id = <0x00>;
                                                port-index = <5>;
                                                bus-width = <CAM_LANES>;
                                                remote-endpoint = <&ecam_csi_out7>;
                                        };
                                };

			};
		};

